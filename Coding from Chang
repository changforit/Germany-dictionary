vocabulary = { "word1": {"meaning": "meaning1", "example_sentence": "example sentence 1", "learned": False}
import random

selected_words = random.sample(vocabulary, quantity) 

def show_new_words(vocabulary, quantity = 5):
  for word in vocabulary:
    if 'learned'== False:
      unknown_words = word
  if len(unknown_words) < quantity:
    print("There are not enough unknown words in the dictionary.")
    return

  for word in selected_words:
    print(f'meaning:{word["meaning"]}')
    print(f'example_sentence:{word["example_sentence"]}\n')
    word["learned"] 0 True
